  "ignoreRegExpList": [
    "zrp",
    "docker",
    "md-link",
    "vuepress-tag",
    "html-tag",
    "slack-tag",
    "emoji"
  ],
  "patterns": [
    {
      "name": "zrp",
      "pattern": "/zrp\\w+/gi"
    },
    {
      "name": "docker",
      "pattern": "/docker\\w+/gi"
    },
    {
      "name": "md-link",
      "pattern": "/\\[.+\\]\\(\\S+\\)/g"
    },
    {
      "name": "vuepress-tag",
      "pattern": "/{{\\W\\$\\S+\\W}}/g"
    },
    {
      "name": "html-tag",
      "pattern": "<(img|iframe)[\\w\\W]+?>"
    },
    {
      "name": "slack-tag",
      "pattern": "@\\S+"
    },
    {
      "name": "emoji",
      "pattern": ":\\S+:"
    }
  ]
}